{
	"name": "relevent-project",
	"displayName": "Show relevant project",
	"description": "Shows the folders which you are working on and hides the other",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:relevent-project.interchangeText"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "relevent-project.interchangeText",
				"title": "SwapText: Swap text between two multicursor locations"
			}
		],
		"keybindings": [
			{
				"command": "relevent-project.interchangeText",
				"key": "ctrl+i",
				"when": "editorHasSelection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.68.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2",
		"ts-loader": "^9.3.0",
		"webpack": "^5.72.1",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.3"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/Teut2711/SwapText.git"
	}
}